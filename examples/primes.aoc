isPrime = fn(n) {
    m = 2
    prime = 1
    while m * m <= n {
        if n % m == 0
            prime = 0
        m = m + 1
    }
    prime
}

findPrimes = fn(n) {
    m = 2
    v = []
    while m <= n {
        if isPrime(m)
            v = v + [m]
        m = m + 1
    }
    v
}

print findPrimes(20)
