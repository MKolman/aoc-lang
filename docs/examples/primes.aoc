isPrime = fn(n) {
    for m = 2; m * m <= n; m = m + 1 {
        if n % m == 0
            return 0
    }
    1
}

findPrimes = fn(n) {
    v = []
    for m = 2; m <= n; m = m + 1 {
        if isPrime(m)
            v = v + [m]
    }
    v
}

print("All the primes up to 20 are:")
print(findPrimes(20))
